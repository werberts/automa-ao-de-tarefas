import pyautogui as posicaodoMause

posicaodoMause.sleep(1)  # tempo para iniciar funçao

# funçao para abrir O GOOGLE  CHROME

posicaodoMause.moveTo(x=227, y=758)
posicaodoMause.doubleClick(x=227, y=758)  # funçao para o mouse clicar na aba
posicaodoMause.sleep(1)  # ESSA FUNÇAO E PARA DEFINIR O TEMPO DE EXECUÇAO


#essa funçao vai abrir o google driver  para anessar os arquivos# print(posicaodoMause.position())


#return (' finalizada... ')texte/cyrela


#essa funçao vai digitar e abrir  gmail
posicaodoMause.sleep(1)  # tempo para iniciar funçao

# funçao para abrir O GOOGLE  CHROME

posicaodoMause.moveTo(x=312, y=77)
posicaodoMause.click(x=312, y=77)
posicaodoMause.sleep(1)

posicaodoMause.typewrite('https://drive.google.com/drive/my-drive')
posicaodoMause.sleep(1)
posicaodoMause.press('enter')




#essa funçao vai carregar o  arquivo para  gogle driver

posicaodoMause.sleep(0.5)
posicaodoMause.moveTo(x=513, y=466)
posicaodoMause.click(x=513, y=466)
posicaodoMause.sleep(1)
#essa funçao vai abrir a pasta  para enviar o arquivo
posicaodoMause.sleep(3)
posicaodoMause.moveTo(x=564, y=201)
posicaodoMause.click(x=564, y=201)
posicaodoMause.sleep(3)
posicaodoMause.typewrite('texte/cyrela')
posicaodoMause.sleep(1)
posicaodoMause.press('enter')

#essa funçao vai abrir a pasta do dryve


posicaodoMause.sleep(3)
posicaodoMause.moveTo(x=587, y=483)
posicaodoMause.doubleClick(x=587, y=483)
posicaodoMause.sleep(3)
